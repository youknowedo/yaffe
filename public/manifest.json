{
  "name": "YAFFE",
  "description": "Yet Another Fast Forward Extension. Play ads 16x faster.",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["main.js"]
    }
  ],
  "action": {
    "default_title": "Yet Another Fast Forward Extension. Play ads 16x faster.",
    "default_icon": {
      "16": "/icons/16x.png",
      "32": "/icons/32x.png",
      "48": "/icons/48x.png",
      "128": "/icons/128x.png"
    }
  },
  "icons": {
    "16": "/icons/16x.png",
    "32": "/icons/32x.png",
    "48": "/icons/48x.png",
    "128": "/icons/128x.png"
  }
}
